	<input>
		<keyboard include="../../Input/Keyboard/carrier-bindings.xml">

			<key n="1">
				<name>CTRL-a</name>
				<desc>Toggle autopilot altitude mode.</desc>
				<binding>
					<command>nasal</command>
					<script>f14_afcs.afcs_altitude_engage_toggle();</script>
				</binding>
			</key>

			<key n="4">
				<name>CTRL-d</name>
				<desc>DLC toggle</desc>
				<binding>
					<command>nasal</command>
					<script>f14.toggleDLC();</script>
				</binding>
			</key>

			<key n="8">
				<name>CTRL-h</name>
				<desc>Toggle autopilot heading mode.</desc>
				<binding>
					<command>nasal</command>
					<script>f14_afcs.afcs_heading_switch(0);</script>
				</binding>
			</key>

			<key n="15">
				<name>Ctrl-o</name>
				<desc>toggle oversweep</desc>
				<binding>
					<command>nasal</command>
					<script>f14.toggleOversweep();</script>
				</binding>
			</key>

			<key n="18">
				<name>Ctrl-r</name>
				<desc>Toggle radar standby mode</desc>
				<binding>
					<command>property-toggle</command>
					<property>instrumentation/radar/radar-standby</property>
				</binding>
			</key>

			<key n="19">
				<name>Ctrl-s</name>
				<desc>Toggle ground spoilers armed</desc>
				<binding>
					<command>nasal</command>
					<script>f14.toggleGroundSpoilers();</script>
				</binding>
			</key>

			<key n="20">
				<name>Ctrl-t</name>
				<desc>Toggle autopilot attitude mode.</desc>
				<binding>
					<command>nasal</command>
					<script>f14_afcs.afcs_engage_toggle()</script>
				</binding>
			</key>

			<key n="22">
				<name>Ctrl-v</name>
				<desc>Select pilot's view (view 0) or toggle between pilot's and RIO's view</desc>
				<binding>
					<command>nasal</command>
					<script>f14.toggle_cockpit_views();</script>
				</binding>
			</key>

			<key n="42">
				<name>*</name>
				<desc>Nose wheel steering or Autopilot selected mode</desc>
				<binding>
					<command>nasal</command>
					<script>
					var wow = getprop ("/gear/gear[1]/wow") or getprop ("/gear/gear[2]/wow");
					if (! wow) {
						f14_afcs.afcs_engage_selected_mode()
					}
					</script>
				</binding>
			</key>

			<key n="68">
				<name>D</name>
				<desc>DLC decrease</desc>
				<binding>
					<command>property-adjust</command>
					<property>/controls/flight/DLC</property>
					<step>-0.1</step>
					<min>0</min>
				</binding>
			</key>

			<key n="69">
				<name>E</name>
				<desc>Decrease Radar Range</desc>
				<binding>
					<command>nasal</command>
					<script>f14.radar_range_control(-1);</script>
				</binding>
			</key>

			<key n="70">
				<name>F</name>
				<desc>Raise flaps</desc>
				<binding>
					<command>nasal</command>
					<script>f14.raiseFlaps();</script>
				</binding>
			</key>

			<key n="82">
				<name>R</name>
				<desc>Increase Radar Range</desc>
				<binding>
					<command>nasal</command>
					<script>f14.radar_range_control(1);</script>
				</binding>
			</key>

			<key n="83">
				<name>S</name>
				<desc>Decrease speed brakes</desc>
				<binding>
					<command>nasal</command>
					<script>f14.speedBrakesIn();</script>
				</binding>
			</key>

			<key n="97">
				<name>a</name>
				<desc>toggle APC</desc>
				<binding>
					<command>nasal</command>
					<script>f14.toggleAPC();</script>
				</binding>
			</key>

			<key n="100">
				<name>d</name>
				<desc>DLC increase</desc>
				<binding>
					<command>property-adjust</command>
					<property>/controls/flight/DLC</property>
					<step>0.1</step>
					<max>1.0</max>
				</binding>
			</key>

			<key n="101">
				<name>e</name>
				<desc>Open canopy and access ladder</desc>
				<binding>
					<command>nasal</command>
					<script>f14.toggleAccess();</script>
				</binding>
			</key>

			<key n="102">
				<name>f</name>
				<desc>Lower flaps</desc>
				<binding>
					<command>nasal</command>
					<script>f14.lowerFlaps();</script>
				</binding>
			</key>

			<key n="104">
				<name>h</name>
				<desc>HSD cycle modes</desc>
				<binding>
					<command>property-adjust</command>
					<property>sim/model/f-14b/controls/pilots-displays/hsd-mode-nav</property>
					<step>1</step>
					<min>-1</min>
					<max>2</max>
					<wrap>true</wrap>
				</binding>
			</key>

			<key n="115">
				<name>s</name>
				<desc>Increase speed brakes</desc>
				<binding>
					<command>nasal</command>
					<script>f14.speedBrakesOut();</script>
				</binding>
			</key>

			<key n="117">
				<name>u</name>
				<desc>Toggle refuelling probe</desc>
				<binding>
					<command>nasal</command>
					<script>f14.toggleProbe();</script>
				</binding>
			</key>

			<key n="362">
				<name>Home</name>
				<desc>Increase elevator trim</desc>
				<binding>
					<command>nasal</command>
					<script>f14.trimUp();</script>
				</binding>
			</key>

			<key n="363">
				<name>End</name>
				<desc>Decrease elevator trim</desc>
				<binding>
					<command>nasal</command>
					<script>f14.trimDown();</script>
				</binding>
			</key>


		</keyboard>
