<PropertyList>

    <path>master-gen.ac</path>

    <animation>
        <type>knob</type>
        <object-name>windshield-air-switch</object-name>
        <property>sim/model/f-14b/controls/windshield-heat</property>

        <factor>-60</factor>
        <offset-deg>30</offset-deg>
        <center>
            <x-m>0.1581</x-m>
            <y-m>-0.0274</y-m>
            <z-m>0.0024</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/f-14b/controls/windshield-heat</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>anti-ice-switch</object-name>
        <property>sim/model/f-14b/controls/anti-ice</property>

        <factor>-45</factor>
        <offset-deg>45</offset-deg>
        <center>
            <x-m>0.1615</x-m>
            <y-m>0.0286</y-m>
            <z-m>0.0024</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/f-14b/controls/anti-ice</property>
                <factor>1</factor>
                <min>0</min>
                <max>2</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>cabin-press-switch</object-name>
        <property>sim/model/f-14b/controls/cabin-pressure</property>

        <factor>-60</factor>
        <offset-deg>30</offset-deg>
        <center>
            <x-m>0.0432</x-m>
            <y-m>-0.054</y-m>
            <z-m>0.0024</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/f-14b/controls/cabin-pressure</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>ram-air-switch</object-name>
        <property>sim/model/f-14b/controls/ram-air</property>

        <factor>-60</factor>
        <offset-deg>30</offset-deg>
        <center>
            <x-m>0.0534</x-m>
            <y-m>0.0112</y-m>
            <z-m>0.0024</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/f-14b/controls/ram-air</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>

    <animation>
        <type>knob</type>
        <object-name>temp-switch</object-name>
        <property>sim/model/f-14b/controls/temp-switch</property>

        <factor>-60</factor>
        <offset-deg>30</offset-deg>
        <center>
            <x-m>0.0432</x-m>
            <y-m>-0.054</y-m>
            <z-m>0.0024</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/f-14b/controls/temp-switch</property>
                <factor>1</factor>
                <min>0</min>
                <max>1</max>
                <wrap>false</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <type>knob</type>
        <object-name>temp-wheel</object-name>
        <property>sim/model/f-14b/controls/cabin-temperature</property>

        <factor>-25.71428571428571</factor>
        <offset-deg>0</offset-deg>
        <center>
            <x-m>0.1096</x-m>
            <y-m>-0.0282</y-m>
            <z-m>-0.0099</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>

        <action>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/f-14b/controls/cabin-temperature</property>
                <factor>1</factor>
                <min>0</min>
                <max>14</max>
                <wrap>true</wrap>
            </binding>
        </action>
    </animation>
    <animation>
        <object-name>temp-wheel</object-name>
        <type>material</type>
        <condition>
            <greater-than>
                <property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
                <value>0</value>
            </greater-than>
            <greater-than-equals>
                <property>controls/lighting/instruments-norm</property>
                <property>controls/lighting/dome-norm</property>
            </greater-than-equals>
            <not>
                <property>/sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <factor-prop>controls/lighting/instruments-norm</factor-prop>
            <red>  1.1000</red>
            <green>0.576</green>
            <blue> 0.161</blue>
            <red-prop>controls/lighting/instrument-red</red-prop>
            <green-prop>controls/lighting/instrument-green</green-prop>
            <blue-prop>controls/lighting/instrument-blue</blue-prop>

        </emission>
    </animation>
    <animation>
        <object-name>temp-wheel</object-name>
        <type>material</type>
        <condition>
            <greater-than>
                <property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
                <value>0</value>
            </greater-than>
            <greater-than>
                <property>controls/lighting/dome-norm</property>
                <property>controls/lighting/instruments-norm</property>
            </greater-than>
            <not>
                <property>/sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <factor-prop>controls/lighting/dome-norm</factor-prop>
            <red>  1.1000</red>
            <green>0.576</green>
            <blue> 0.161</blue>
            <red-prop>controls/lighting/instrument-red</red-prop>
            <green-prop>controls/lighting/instrument-green</green-prop>
            <blue-prop>controls/lighting/instrument-blue</blue-prop>
        </emission>
    </animation>
    <!-- bleed air source -->
    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>air-source-off-button</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/systems/ecs/bleed-air-source</property>
                <value>0</value>
            </binding>
        </action>
    </animation>
    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>air-source-leng-button</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/systems/ecs/bleed-air-source</property>
                <value>1</value>
            </binding>
        </action>
    </animation>
    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>air-source-reng-button</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/systems/ecs/bleed-air-source</property>
                <value>2</value>
            </binding>
        </action>
    </animation>
    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>air-source-both-button</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/systems/ecs/bleed-air-source</property>
                <value>3</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>air-source-ram-button</object-name>
        <action>
            <button>0</button>
            <binding>
                <command>property-assign</command>
                <property>fdm/jsbsim/systems/ecs/bleed-air-source</property>
                <value>4</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>air-source-leng-button</object-name>
        <property>fdm/jsbsim/systems/ecs/bleed-air-source</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.003</dep>
            </entry>
            <entry>
                <ind>2</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-0.001</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>air-source-reng-button</object-name>
        <property>fdm/jsbsim/systems/ecs/bleed-air-source</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>2</ind>
                <dep>0.003</dep>
            </entry>
            <entry>
                <ind>3</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-0.001</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>air-source-both-button</object-name>
        <property>fdm/jsbsim/systems/ecs/bleed-air-source</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>2</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>3</ind>
                <dep>0.003</dep>
            </entry>
            <entry>
                <ind>4</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-0.001</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>air-source-ram-button</object-name>
        <property>fdm/jsbsim/systems/ecs/bleed-air-source</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>3</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>4</ind>
                <dep>0.003</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-0.001</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>air-source-off-button</object-name>
        <property>fdm/jsbsim/systems/ecs/bleed-air-source</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0.003</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-0.001</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>emerg-gen-guard-lever</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>sim/model/f-14b/controls/electrics/emerg-gen-guard-lever</property>
            </binding>
            <binding>
                <command>property-assign</command>
                <property>sim/model/f-14b/controls/electrics/emerg-gen-switch</property>
                <value>1</value>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>l-gen-switch</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>sim/model/f-14b/controls/electrics/l-gen-switch</property>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <object-name>r-gen-switch</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>sim/model/f-14b/controls/electrics/r-gen-switch</property>
            </binding>
        </action>
    </animation>

    <animation>
        <type>pick</type>
        <visible>true</visible>
        <condition>
            <property>sim/model/f-14b/controls/electrics/emerg-gen-guard-lever</property>
        </condition>

        <object-name>emerg-gen-switch</object-name>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-toggle</command>
                <property>sim/model/f-14b/controls/electrics/emerg-gen-switch</property>
            </binding>
        </action>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>l-gen-switch</object-name>
        <property>sim/model/f-14b/controls/electrics/l-gen-switch</property>
        <factor>-50</factor>
        <center>
            <x-m>-0.0204</x-m>
            <y-m>-0.0434</y-m>
            <z-m> 0.0037</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>r-gen-switch</object-name>
        <property>sim/model/f-14b/controls/electrics/r-gen-switch</property>
        <factor>-50</factor>
        <center>
            <x-m>-0.0204</x-m>
            <y-m>-0.0049</y-m>
            <z-m> 0.0037</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>emerg-gen-switch</object-name>
        <property>sim/model/f-14b/controls/electrics/emerg-gen-switch</property>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>50</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-0.0258</x-m>
            <y-m> 0.0615</y-m>
            <z-m> 0.0037</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>emerg-gen-guard-lever</object-name>
        <property>sim/model/f-14b/controls/electrics/emerg-gen-guard-lever</property>
        <factor>90</factor>
        <center>
            <x-m>-0.0042</x-m>
            <y-m> 0.0618</y-m>
            <z-m> 0.0045</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <!-- lighting -->
    <animation>
        <object-name>mount</object-name>
        <object-name>switches</object-name>
        <type>material</type>
        <condition>
            <greater-than>
                <property>/fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
                <value>0</value>
            </greater-than>
            <not>
                <property>sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <factor-prop>controls/lighting/dome-norm</factor-prop>
            <red>  1.000</red>
            <green>0.576</green>
            <blue> 0.161</blue>
            <red-prop>controls/lighting/dome-red</red-prop>
            <green-prop>controls/lighting/dome-green</green-prop>
            <blue-prop>controls/lighting/dome-blue</blue-prop>
        </emission>
    </animation>

    <!-- implicit ALS lighting see http://wiki.flightgear.org/ALS_technical_notes -->
    <effect>
        <name>f14panel</name>
        <inherits-from>Effects/f14panel</inherits-from>
        <object-name>mount</object-name>
    </effect>
</PropertyList>
