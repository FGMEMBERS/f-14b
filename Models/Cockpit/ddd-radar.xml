	</MP1>

	<animation>
		<type>select</type>
		<object-name alias="../../MP1/object-name"/>
		<condition>
			<equals>
				<property alias="../../../../MP1/in-range"/>
				<value>true</value>
			</equals>
			<equals>
				<property alias="../../../../MP1/display"/>
				<value>true</value>
			</equals>
		</condition>
	</animation>
	<animation> 
		<type>translate</type>
		<object-name alias="../../MP1/object-name"/>
		<property alias="../../MP1/ddd-relative-bearing"/>
		<axis><y>1</y></axis>
	</animation>
	<animation>
		<type>translate</type>
		<object-name alias="../../MP1/object-name"/>
		<property alias="../../MP1/ddd-draw-range"/>
		<condition>
			<not><property>instrumentation/radar/radar-standby</property></not>
		</condition>
		<axis><z>1</z></axis>
	</animation>
	<animation>
		<object-name alias="../../MP1/object-name"/>
		<type>material</type>
		<emission>
			<factor-prop>sim/model/f-14b/controls/radar-awg-9/brightness</factor-prop>
			<red>0.0</red>
			<green>1</green>
			<blue>0.0</blue>
		</emission>
		<transparency>
			<alpha-prop alias="../../../MP1/ddd-echo-fading"/>
		</transparency>
	</animation>

