<?xml version="1.0"?>

<!--Simple Tactical Display (Basic FG Radar) -->

<PropertyList>

	<path>ddd-radar.ac</path>

	<!-- don't show it when external view or Pilot View-->
	<animation>
		<type>select</type>
		<object-name>ddd</object-name>
		<condition>
			<equals>
				<property>sim/current-view/name</property>
				<value>RIO View</value>
			</equals>
		</condition>
	</animation>

	<!-- screen background -->
	<animation>
		<type>select</type>
		<object-name>screen-bkg</object-name>
		<condition>
			<property>sim/model/f-14b/controls/radar-awg-9/on-off</property>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>screen-bkg-off</object-name>
		<condition>
			<not>
				<property>sim/model/f-14b/controls/radar-awg-9/on-off</property>
			</not>
		</condition>
	</animation>

	<animation>
		<type>select</type>
		<object-name>radar-sweep</object-name>
		<condition>
			<property>sim/model/f-14b/instrumentation/radar-awg-9/display-rdr</property>
		</condition>
	</animation>
	<animation>
		<name>radar-sweep</name>
		<type>translate</type>
		<object-name>radar-sweep</object-name>
		<property>sim/model/f-14b/instrumentation/awg-9/sweep-factor</property>
		<factor>0.0411</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-0.0105</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>1</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<name>horizon-line-roll-rotation</name>
		<type>rotate</type>
		<object-name>horizon-line</object-name>
		<property>orientation/roll-deg</property>
		<factor>1</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>-0.0105</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>
	<animation>
		<name>horizon-line-pitch-translation</name>
		<type>translate</type>
		<object-name>horizon-line</object-name>
		<property>orientation/pitch-deg</property>
		<factor>0.00218</factor>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>0</y>
			<z>-1</z>
		</axis>
	</animation>
