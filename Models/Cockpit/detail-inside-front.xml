<PropertyList>

	<!-- DÃ©tailled Cockpit Consoles and Panels -->
	<name>Detail-Inside-Front</name>
	<path>detail-inside-front.ac</path>

	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>fire-handle-l</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>f14.fire_handle(0)</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>fire-handle-l</object-name>
		<property>controls/engines/engine[0]/cutoff</property>
		<factor>0.05</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>pick</type>
		<visible>true</visible>
		<object-name>fire-handle-r</object-name>
		<action>
			<button>0</button>
			<binding>
				<command>nasal</command>
				<script>f14.fire_handle(1)</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>translate</type>
		<object-name>fire-handle-r</object-name>
		<property>controls/engines/engine[1]/cutoff</property>
		<factor>0.05</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>L-pedal</object-name>
		<property>controls/flight/rudder</property>
		<factor>0.07</factor>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>translate</type>
		<object-name>R-pedal</object-name>
		<property>controls/flight/rudder</property>
		<factor>0.07</factor>
		<axis>
			<x>-1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<!--<animation> 
		<object-name>mount</object-name>
		<type>material</type>
		<emission>
			<factor-prop>controls/lighting/panel-norm</factor-prop>
			<red>0.55</red>
			<green>0.24</green>
			<blue>0.10</blue>
		</emission>
	</animation>-->
	<animation> 
		<object-name>radio-labels</object-name>
		<object-name>vert-side-panels</object-name>
		<object-name>panel-front-up</object-name>
		<object-name>knee-panels</object-name>
		<object-name>pedestal</object-name>
		<object-name>consoles</object-name>
		<object-name>checklists-pilot</object-name>
        <object-name>pedals</object-name>
        <object-name>bathtub-sides</object-name>
        <object-name>fire-handle-l</object-name>
        <object-name>fire-handle-r</object-name>
        <object-name>floor</object-name>
        <type>material</type>
        <condition>
            <greater-than>
                <property>fdm/jsbsim/systems/electrics/ac-essential-bus1</property>
                <value>0</value>
            </greater-than>
            <not>
                <property>/sim/rendering/rembrandt/enabled</property>
            </not>
        </condition>
        <emission>
            <factor-prop>controls/lighting/dome-norm</factor-prop>
            <red>  1.000</red>
            <green>0.576</green>
            <blue> 0.161</blue>
            <red-prop>controls/lighting/panel-red</red-prop>
            <green-prop>controls/lighting/panel-green</green-prop>
            <blue-prop>controls/lighting/panel-blue</blue-prop>

        </emission>
    </animation>

    
    <effect>
        <name>f14panel</name>
        <inherits-from>Aircraft/f-14b/Models/Effects/f14panel</inherits-from>
        <object-name>radio-labels</object-name>
    </effect>
    <effect>
        <name>f14-interior</name>
        <inherits-from>Aircraft/f-14b/Models/Effects/f14interior</inherits-from>
        <object-name>inside-front</object-name>
        <object-name>floor</object-name>
        <object-name>consoles</object-name>
        <object-name>bathtub-sides</object-name>
        <object-name>vert-side-panels</object-name>
        <object-name>pedals</object-name>
        <object-name>L-pedal</object-name>
        <object-name>R-pedal</object-name>
        <object-name>radio-labels</object-name>
        <object-name>pedestal</object-name>
        <object-name>panel-front-up</object-name>
        <object-name>knee-panels</object-name>
        <object-name>fire-handle-r</object-name>
        <object-name>fire-handle-l</object-name>
        <object-name>checklists-pilot</object-name>
    </effect>
</PropertyList>
