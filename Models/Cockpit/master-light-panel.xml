<?xml version="1.0"?>

	<PropertyList>

	<path>master-light-panel.ac</path>

	<!-- Hook Bypass Switch -->
	<animation>
		<type>rotate</type>
		<object-name>hook-bypass-switch</object-name>
		<property>sim/model/f-14b/controls/lighting/hook-bypass</property>
		<factor>33</factor>
		<center>
			<x-m>-0.0507</x-m>
			<y-m>0.0</y-m>
			<z-m>0.0038</z-m>
		</center>
		<axis>
			<x>0</x>
			<y>-1</y>
			<z>0</z>
		</axis>
	</animation>

