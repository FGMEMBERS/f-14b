	<!-- instrument-wheel -->
	<animation>
		<type>rotate</type>
		<object-name>instrument-wheel</object-name>
		<property>controls/lighting/instruments-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>336</dep></entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0.0002</y-m>
			<z-m>-0.0100</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	<animation>
		<type>rotate</type>
		<object-name>console-wheel</object-name>
		<property>controls/lighting/panel-norm</property>
		<interpolation>
			<entry><ind>0</ind><dep>0</dep></entry>
			<entry><ind>1</ind><dep>336</dep></entry>
		</interpolation>
		<center>
			<x-m>0</x-m>
			<y-m>0.0002</y-m>
			<z-m>-0.0100</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
	</animation>

	

