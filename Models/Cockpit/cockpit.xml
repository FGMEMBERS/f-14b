<?xml version="1.0"?>


<PropertyList>

	<path>group-cockpit.ac</path>



<!-- De-select Pilot when in Cockpit View -->
	<animation>
		<type>select</type>
		<object-name>Pilot</object-name>
		<object-name>RIO</object-name>
		<condition>
			<not>
				<equals>
					<property>sim/current-view/name</property>
					<value>Cockpit View</value>
				</equals>
			</not>
		</condition>
	</animation>

<!-- ******************************************************************* -->
