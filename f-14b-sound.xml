		</wind> -->

		<!--<stall-warning>
			<name>stall-warning</name>
			<path>Sounds/stall.wav</path>
			<property>sim/alarms/stall-warning</property>
			<volume>
				<property>velocities/airspeed-kt</property>
			<max>0.3</max>
			</volume>
			<pitch>
				<max>0.5</max>
			</pitch>
		</stall-warning> -->

		<low-alt-warning>
			<name>stall-warning</name>
			<path>Aircraft/f-14b/Sounds/low-alt-warning.wav</path>
			<mode>once</mode>
			<volume>
				<factor>0.25</factor>
			</volume>
			<condition>
				<less-than>
					<property>position/altitude-agl-ft</property>
					<property>sim/model/f-14b/instrumentation/radar-altimeter/limit-bug</property>
				</less-than>
			</condition>
		</low-alt-warning>

		<!-- Master Light Panel -->
		<hook-bypass-switch-on>
			<name>hook-bypass-switch-on</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/lighting/hook-bypass</property>
					<value>true</value>
				</equals>
			</condition>
			<volume> 
				<factor>0.15</factor>
			</volume>
		</hook-bypass-switch-on>
		<hook-bypass-switch-off> 
			<name>hook-bypass-switch-off</name>
			<mode>once</mode>
			<path>Aircraft/f-14b/Sounds/click.wav</path>
			<condition> 
				<equals> 
					<property>sim/model/f-14b/controls/lighting/hook-bypass</property>
					<value>false</value>
				</equals>
			</condition>
			<volume> 
				<factor>0.15</factor>
			</volume>
		</hook-bypass-switch-off>

	</fx>

</PropertyList>
