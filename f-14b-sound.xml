<?xml version="1.0"?>
 
<PropertyList>

	<fx>
		<!-- Engines -->
		<engine-jet>
			<name>engine-jet</name>
			<path>Sounds/jet.wav</path>
			<mode>looped</mode>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<not>
					<property>engines/engine/out-of-fuel</property>
				</not>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>180.0</z>
				<inner-cone>180.0</inner-cone>
				<outer-cone>360.0</outer-cone>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine/n1</property>
				<ofset>0.1</ofset>
				<factor>.001</factor>
			</pitch>
			<volume>
				<property>engines/engine[0]/thrust-lbs</property>
				<factor>0.000025</factor>
				<min>0.2</min>
				<max>0.75</max>
			</volume>
		</engine-jet>

		<engine-rumble>
			<name>engine-rumble</name>
			<path>Sounds/rumble.wav</path>
			<mode>looped</mode>
			<property>engines/engine[0]/afterburner</property>
			<orientation>
				<x>0.0</x>
				<y>0.0</y>
				<z>180.0</z>
				<inner-cone>180.0</inner-cone>
				<outer-cone>360.0</outer-cone>
				<outer-gain>0.2</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine[0]/afterburner</property>
				<factor>0.01</factor>
				<offset>0.15</offset>
			</pitch>
			<volume>
				<property>engines/engine[0]/afterburner</property>
				<factor>0.7</factor>
				<offset>0.3</offset>
				<max>1</max>
			</volume>
		</engine-rumble>

		<engine-whine-external>
			<name>external-whine</name>
			<path>Sounds/turbojet_fan.wav</path>
			<mode>looped</mode>
			<condition>
				<not>
					<property>sim/current-view/internal</property>
				</not>
				<not>
					<property>engines/engine/out-of-fuel</property>
				</not>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>-5.0</y>
				<z>0.0</z>
				<inner-cone>50.0</inner-cone>
				<outer-cone>180.0</outer-cone>
				<outer-gain>0.0</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine/n1</property>
				<ofset>0.7</ofset>
				<factor>.003</factor>
			</pitch>
			<volume>
				<property>engines/engine/n1</property>
				<offset>1.0</offset>
				<factor>-0.008</factor>
				<max>0.6</max>
			</volume>
		</engine-whine-external>

		<engine-whine-internal>
			<name>internal-whine</name>
			<path>Sounds/whine.wav</path>
			<mode>looped</mode>
			<condition>
				<property>sim/current-view/internal</property>
				<property>engines/engine/running</property>
			</condition>
			<orientation>
				<x>0.0</x>
				<y>-5.0</y>
				<z>0.0</z>
				<inner-cone>50.0</inner-cone>
				<outer-cone>180.0</outer-cone>
				<outer-gain>0.0</outer-gain>
			</orientation>
			<pitch>
				<property>engines/engine/n1</property>
				<ofset>0.1</ofset>
				<factor>.0016</factor>
			</pitch>
			<volume>
				<property>engines/engine[0]/thrust-lbs</property>
				<factor>0.000012</factor>
				<min>0.2</min>
				<max>0.7</max>
			</volume>
		</engine-whine-internal>

		<!-- Rumble -->
		<rumble>
			<name>rumble</name>
			<mode>looped</mode>
			<path>Sounds/rumble.wav</path>
			<condition>
				<or>
					<property>gear/gear[0]/wow</property>
					<property>gear/gear[1]/wow</property>
					<property>gear/gear[2]/wow</property>
				</or>
			</condition>
			<volume>
				<property>/velocities/airspeed-kt</property>
				<factor>.002</factor>
			</volume>
		</rumble>

		<!-- Gears -->
		<gear-transit>
			<name>gear-transit</name>
			<mode>in-transit</mode>
			<path>Sounds/gear.wav</path>
			<property>gear/gear[0]/position-norm</property>
			<pitch>
				<internal>dt_play</internal>
				<offset>1.0</offset>
				<factor>-0.001</factor>
			</pitch>
			<volume>
				<factor>0.20</factor>
			</volume>
		</gear-transit>

		<gear-lock>
			<name>gear-lock</name>
			<path>Sounds/gear-lck.wav</path>
			<condition>
				<equals>
					<property>gear/gear[0]/position-norm</property>
					<value>0</value>
				</equals>
			</condition>
			<volume>
				<factor>0.25</factor>
			</volume>
		</gear-lock>

		<gear-squeal>
			<name>gear-squeal</name>
			<path>Sounds/squeal.wav</path>
			<condition>
				<or>
					<property>gear/gear[0]/wow</property>
					<property>gear/gear[1]/wow</property>
					<property>gear/gear[2]/wow</property>
				</or>
			</condition>
			<volume>
				<property>velocities/speed-down-fps</property>
				<factor>0.05</factor>
			</volume>
			<volume>
				<property>velocities/airspeed-kt</property>
				<factor>0.01</factor>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>velocities/airspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.2</offset>
			</pitch>
		</gear-squeal>

		<flap-in-transit>
			<name>flaps-in-transit</name>
			<mode>in-transit</mode>
			<path>Sounds/flaps.wav</path>
			<property>surface-positions/main-flap-pos-norm</property>
			<pitch>
				<property>surface-positions/main-flap-pos-norm</property>
				<factor>-0.1</factor>
			</pitch>
			<volume>
				<factor>0.2</factor>
			</volume>
		</flap-in-transit>

