 <!-- =============================================================== -->
 <!-- Stability Augmentation System controllers                       -->
 <!-- =============================================================== -->

  <!-- Pitch SAS -->

  <pid-controller>
    <name>Pitch stabilisation</name>
    <debug>false</debug>
    <enable>
	  <prop>/f-14/SAS/engaged</prop>
      <value>true</value>
    </enable>
    <input>
      <prop>/orientation/phi-dot</prop>
    </input>
    <reference>
       <value>0.0</value> 
    </reference>
    <output>
		<prop>/f-14/SAS/pitch-bias</prop>
    </output>
    <config>
	  <Kp>-0.05</Kp>      <!-- proportional gain -->
	  <Ki>0.0</Ki>
	  <Kd>0.0</Kd>
      <Ti>1.0</Ti>        <!-- integrator time -->
      <Td>0.00001</Td>    <!-- derivator time -->
      <u_min>-0.2</u_min> <!-- minimum output clamp -->
      <u_max>0.2</u_max>  <!-- maximum output clamp -->
    </config>
  </pid-controller>

  <!-- Yaw damper -->

  <pid-controller>
    <name>yaw damper</name>
    <debug>false</debug>
    <enable>
	  <prop>/f-14/SAS/engaged</prop>
      <value>true</value>
    </enable>
    <input>
      <prop>/orientation/side-slip-deg</prop>
    </input>
    <reference>
       <value>0.0</value> 
    </reference>
    <output>
		<prop>/f-14/SAS/yaw-bias</prop>
    </output>
    <config>
	  <Kp>0.03</Kp>      <!-- proportional gain -->
	  <Ki>0.0</Ki>
	  <Kd>0.5</Kd>
      <Ti>1.0</Ti>        <!-- integrator time -->
      <Td>0.00001</Td>    <!-- derivator time -->
      <u_min>-0.3</u_min> <!-- minimum output clamp -->
      <u_max>0.3</u_max>  <!-- maximum output clamp -->
    </config>
  </pid-controller>



</PropertyList>
